{
    "$schema": "https://docs.tolgee.io/cli-schema.json",
    "projectId": 2,
    "format": "JSON_TOLGEE",
    "patterns": [
        "./src/**/*.ts?(x)"
    ],
    "push": {
        "filesTemplate": "./src/translations/{languageTag}.json",
        "language": [
            "pt-BR",
            "en"
        ]
    },
    "pull": {
        "path": "./src/translations"
    }
}